t: center;\r\n      }\r\n      .card {\r\n        border: 1px solid $blueOwlBlue;\r\n        height: 200px;\r\n        margin: 0 20px 20px 0;\r\n        position: relative;\r\n        border-radius: 6px;\r\n        background-color: $blueOwlBlue;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        @media (max-width: 600px) {\r\n          width: 100%;\r\n          margin: 0;\r\n        }\r\n        p {\r\n          color: white;\r\n          font-size: 45px;\r\n        }\r\n        i {\r\n          position: absolute;\r\n          top: 5px;\r\n          right: 5px;\r\n          opacity: 0.6;\r\n          transition: all 0.3s ease-in-out;\r\n          height: 20px;\r\n          width: 20px;\r\n          padding: 10px;\r\n          background: $blueOwlBlue;\r\n          border-radius: 50%;\r\n          display: flex;\r\n          justify-content: center;\r\n          align-items: center;\r\n          padding: 5px;\r\n          color: white;\r\n          &:hover {\r\n            opacity: 1;\r\n            cursor: pointer;\r\n            background: white;\r\n            color: $blueOwlBlue;\r\n          }\r\n        }\r\n      }\r\n    }\r\n    .action-bar {\r\n      padding: 10px 20px;\r\n      background: $lightGrey;\r\n      display: flex;\r\n      height: 40px;\r\n     \r\n      button {\r\n        border: 0;\r\n        border-radius: 6px;\r\n        background: $blueOwlBlue;\r\n        color: white;\r\n        padding: 10px 20px;\r\n        width: 185px;\r\n        opacity: 0.8;\r\n        transition: all 0.3s ease-in-out;\r\n        user-select: none;\r\n        outline: none;\r\n        margin: 0 10px 0 0;\r\n        &:hover {\r\n          opacity: 1;\r\n          cursor: pointer;\r\n          \r\n        }\r\n      }\r\n      #card-count {\r\n        margin: 0 0 0 auto;\r\n        background: $blueOwlBlue;\r\n        border-radius: 50%;\r\n        height: 20px;\r\n        width: 20px;\r\n        color: white;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        padding: 10px;\r\n        @media (max-width: 600px) {\r\n\r\n           display: none; \r\n        }\r\n      }\r\n      @media (max-width: 600px) {\r\n        height: auto; \r\n        button {\r\n            opacity: 1;\r\n            i {\r\n                display: none;\r\n            }\r\n            &:nth-child(3) {\r\n                margin-right: 0;\r\n            }\r\n        }\r\n   }\r\n    }\r\n  }\r\n  .right {\r\n    width: 400px;\r\n    border-left: 2px solid $blueOwlBlue;\r\n    padding: 20px;\r\n    max-height: 100vh;\r\n    overflow-y: auto;\r\n    @include scrollbar;\r\n    @media (max-width: 768px) {\r\n      display: none;\r\n    }\r\n    h3 {\r\n      font-weight: bold;\r\n      text-align: center;\r\n      margin: 0 0 10px 0;\r\n    }\r\n    p {\r\n      line-height: 1.6;\r\n      margin: 0 0 10px 0;\r\n      width: 100%;\r\n      font-weight: 100;\r\n    }\r\n  }\r\n}\r\n","src\\styles\\_mixins.scss":"@mixin scrollbar() {\r\n  /* width */\r\n  &::-webkit-scrollbar {\r\n    width: 15px;\r\n  }\r\n\r\n  /* Track */\r\n  &::-webkit-scrollbar-track {\r\n    background:$lightGrey;\r\n  }\r\n\r\n  /* Handle */\r\n  &::-webkit-scrollbar-thumb {\r\n    background: $blueOwlBlue;\r\n  }\r\n  &::-webkit-scrollbar-thumb:hover {\r\n    cursor: pointer;\r\n  }\r\n}\r\n","src\\styles\\_variables.scss":"$blueOwlBlue: #3473ff;\r\n$lightGrey: #F7F7F9;"},"lineCount":null}},"error":null,"hash":"eb32752188c003fd7fc8f026bc283adf","cacheData":{"env":{}}}